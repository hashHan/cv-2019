{
  "name": "cv-2019",
  "version": "0.1.0",
  "description": "Haeseong Han's cv made in 2019",
  "main": "./server/index.js",
  "private": true,
  "repository": "https://github.com/hashHan/cv-2019.git",
  "author": "Haeseong <haeseong.han@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:development": "cross-env REACT_APP_CUSTOM_ENV=development yarn start",
    "start:staging": "cross-env REACT_APP_CUSTOM_ENV=staging yarn start",
    "start:production": "cross-env REACT_APP_CUSTOM_ENV=production yarn start",
    "configSW": "sw-precache --config=src/sw-precache-config.js",
    "build": "react-scripts build",
    "build:development": "cross-env REACT_APP_CUSTOM_ENV=development yarn build && yarn configSW",
    "build:staging": "cross-env REACT_APP_CUSTOM_ENV=staging yarn build && yarn configSW",
    "build:production": "cross-env REACT_APP_CUSTOM_ENV=production yarn build && yarn configSW",
    "serve": "cross-env NODE_ENV=production REACT_APP_CUSTOM_ENV=production node ./server/index.js",
    "serve:debug": "cross-env NODE_ENV=development REACT_APP_CUSTOM_ENV=development nodemon ./server/index.js",
    "serve:development": "cross-env NODE_ENV=production REACT_APP_CUSTOM_ENV=development nodemon ./server/index.js",
    "serve:staging": "cross-env NODE_ENV=production REACT_APP_CUSTOM_ENV=staging nodemon ./server/index.js",
    "serve:production": "cross-env NODE_ENV=production REACT_APP_CUSTOM_ENV=production nodemon ./server/index.js",
    "local:debug": "yarn build:development && yarn serve:debug",
    "local:development": "yarn build:development && yarn serve:development",
    "local:staging": "yarn build:staging && yarn serve:staging",
    "local:production": "yarn build:production && yarn serve:production",
    "deploy:development": "pm2 deploy ecosystem.config.js development",
    "deploy:staging": "pm2 deploy ecosystem.config.js staging",
    "deploy:production": "pm2 deploy ecosystem.config.js production"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "bluebird": "^3.5.3",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "connected-react-router": "^4.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "deepmerge": "^3.1.0",
    "express-http-proxy": "^1.5.1",
    "express-robots-txt": "^0.4.0",
    "forcedomain": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "immutable": "^4.0.0-rc.12",
    "js-cookie": "^2.2.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-slack": "^0.0.7",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "with-immutable-props-to-js": "^1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^1.1.0",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0"
  }
}
